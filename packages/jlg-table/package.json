{
	"name": "jlg-table",
	"private": false,
	"version": "0.0.1",
	"type": "commonjs",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"prettier:fix": "prettier --write .",
		"eslint:fix": "eslint --fix .",
		"pre-commit": "concurrently \"tsc\" \"lint-staged\"",
		"preinstall": "npx only-allow pnpm"
	},
	"files": [
		"dist",
		"lib",
		"styles",
		"global.d.ts"
	],
	"sideEffects": [
		"./dist/style.css"
	],
	"main": "dist/jlg-table.umd.js",
	"module": "dist/jlg-table.es.js",
	"types": "dist/packages/index.d.ts",
	"style": "dist/style.css",
	"exports": {
		".": {
			"import": "./dist/jlg-table.es.js",
			"require": "./dist/jlg-table.umd.js",
			"types": "./dist/packages/index.d.ts"
		},
		"./dist/":  {
			"import": "./dist/",
			"require": "./dist/"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"license": "MIT",
	"keyword": [
		"vue3",
		"element-plus",
		"vxe-table",
		"grid",
		"modal"
	],
	"description": "基于element-plus + vxe-table 二次封装的UI组件库，支持增删改查、虚拟列表、虚拟树、懒加载、快捷菜单、数据校验、树形结构、打印导出、表单渲染、数据分页、弹窗、自定义模板、渲染器、JSON 配置式",
	"repository": {
		"type": "git",
		"url": "http://10.1.2.5/front-end-dependency/component/component-library/monorepo-component-plus.git"
	},
	"peerDependencies": {
		"vue": "^3.4.21",
		"vxe-table": "^4.0.0",
		"xe-utils": "^3.5.26",
		"element-plus": "^2.7.0",
		"sortablejs": "^1.15.2",
		"@element-plus/icons-vue": "^2.3.1",
		"@types/sortablejs": "^1.15.8"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/eslint-parser": "^7.22.15",
		"@element-plus/icons-vue": "^2.3.1",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "7.0.0",
		"@typescript-eslint/parser": "7.0.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"axios": "^1.6.8",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-vue": "^9.18.1",
		"lint-staged": "^15.0.2",
		"postcss": "^8.4.38",
		"postcss-html": "^1.6.0",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.2.5",
		"sass": "^1.76.0",
		"sortablejs": "^1.15.2",
		"stylelint": "^16.5.0",
		"stylelint-config-recommended-scss": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"stylelint-order": "^6.0.0",
		"stylelint-scss": "^6.3.0",
		"typescript": "^5.4.0",
		"unplugin-auto-import": "^0.17.5",
		"unplugin-vue-components": "^0.26.0",
		"unplugin-vue-define-options": "^1.4.3",
		"vite": "^5.2.0",
		"vite-plugin-dts": "^3.9.0",
		"vite-plugin-eslint": "^1.8.1",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^2.0.0",
		"vxe-table": "^4.6.9",
		"xe-utils": "^3.5.26"
	}
}
